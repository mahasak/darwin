{
  "name": "darwin",
  "version": "1.0.0",
  "description": "Darwin Opensource Commerce",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "NODE_ENV=production  tsc --build tsconfig.json ",
    "build:client": "./node_modules/.bin/webpack --config ./src/webpack.config.js --progress",
    "build:server": "tsc",
    "start:dev": "npm run build:client && nodemon --config ./nodemon.json",
    "dev": "tsc-watch --onSuccess \"node ./dist/server/server.js\"",
    "start:prod": "node dist/server/server.js",
    "start": "cross-env NODE_ENV=production REACT_SSR_ENV=production node dist/server.js",
    "test": "jest --coverage --verbose --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahasak/darwin.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mahasak/darwin/issues"
  },
  "homepage": "https://github.com/mahasak/darwin#readme",
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
