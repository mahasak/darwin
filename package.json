{
  "name": "darwin",
  "version": "1.0.0",
  "description": "Darwin Opensource Commerce",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "NODE_ENV=production REACT_SSR_ENV=production tsc --build tsconfig.server.json ",
    "dev": "tsc-watch --onSuccess \"node ./dist/server/server.js\"",
    "start": "cross-env NODE_ENV=production REACT_SSR_ENV=production node dist/server/server.js",
    "test": "jest --coverage --verbose --passWithNoTests"
  },
